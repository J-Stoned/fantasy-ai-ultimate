# MARCUS SECURE ENV TEMPLATE
# Copy this to .env.local and fill in your actual values
# NEVER commit .env or .env.local to version control!

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
DATABASE_URL=postgresql://postgres:your-password@localhost:5432/fantasy_ai

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Sports APIs
BALLDONTLIE_API_KEY=your-balldontlie-key
MYSPORTSFEEDS_API_KEY=your-mysportsfeeds-key
SPORTRADAR_API_KEY=your-sportradar-key

# Fantasy Platform OAuth
YAHOO_CLIENT_ID=your-yahoo-client-id
YAHOO_CLIENT_SECRET=your-yahoo-client-secret
ESPN_API_KEY=your-espn-api-key
SLEEPER_API_KEY=not-required

# Stripe Payment Integration
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# AI Services
OPENAI_API_KEY=sk-your-openai-api-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key
ELEVENLABS_API_KEY=your-elevenlabs-key
REPLICATE_API_TOKEN=your-replicate-token

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=generate-with-openssl-rand-base64-32

# Google Services
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# External Services
SENTRY_DSN=your-sentry-dsn
SENTRY_ORG=your-org
SENTRY_PROJECT=your-project

# Feature Flags
ENABLE_GPU_ACCELERATION=true
ENABLE_WEBSOCKET=true
ENABLE_REDIS_CACHE=true
ENABLE_PATTERN_DETECTION=true

# Environment
NODE_ENV=development