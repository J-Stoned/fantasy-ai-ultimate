  "license": "MIT",
  "scripts": {
    "test:db": "tsx scripts/test-db-connection.ts",
    "dev": "node --max-old-space-size=4096 node_modules/.bin/nx run-many --target=serve --all",
    "dev:web": "tsx scripts/start-web-app.ts",
    "dev:web:nx": "nx serve web",
    "dev:mobile": "nx serve mobile",
    "build": "node --max-old-space-size=4096 node_modules/.bin/nx run-many --target=build --all",
    "build:web": "nx build web",
    "build:mobile": "nx build mobile",
    "start:web": "cd dist/web && node server.js",
    "test": "node --max-old-space-size=4096 node_modules/.bin/nx run-many --target=test --all",
    "test:unit": "node --max-old-space-size=4096 node_modules/.bin/nx run-many --target=test --all",
    "test:integration": "node --max-old-space-size=4096 node_modules/.bin/nx run-many --target=test --all --configuration=integration",
    "test:e2e": "nx e2e web-e2e",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "lint": "nx run-many --target=lint --all",
    "type-check": "tsc --noEmit",
    "security:check": "tsx scripts/security-check.ts",
    "security:fix": "tsx scripts/enable-comprehensive-rls.sql",
    "load:test": "tsx scripts/load-test-100k-users.ts",
    "load:test:local": "LOAD_TEST_URL=http://localhost:3000 tsx scripts/load-test-100k-users.ts",
    "load:test:prod": "LOAD_TEST_URL=$PRODUCTION_URL tsx scripts/load-test-100k-users.ts",
    "prod:verify": "npm run security:check && npm run test && npm run build",
    "prod:deploy": "npm run prod:verify && npm run deploy",
    "format": "nx format:write",
    "migrate": "tsx scripts/run-migrations.ts",
    "seed": "tsx scripts/run-seed.ts",
    "ml:train": "tsx scripts/train-ml-models-gpu.ts",
    "data:mega-collect": "tsx scripts/mega-data-collector.ts",
    "fantasy:activate": "tsx scripts/launch-fantasy-ai.ts",
    "gpu:enable": "tsx scripts/enable-gpu-acceleration.ts",
    "data:simple": "tsx scripts/simple-data-collector.ts",
    "data:auto": "tsx scripts/auto-collect.ts",
    "data:monitor": "tsx scripts/monitor-data-growth.ts",
    "data:check": "tsx scripts/check-all-tables.ts",
    "turbo": "tsx scripts/turbo-loader.ts",
    "turbo:fantasy": "tsx scripts/fantasy-turbo-loader.ts",
    "clean": "nx reset && rm -rf dist .next node_modules/.cache",
    "load-test": "tsx lib/testing/load-test.ts",
    "load-test:k6": "k6 run lib/testing/k6-nfl-sunday.js",
    "load-test:k6:cloud": "k6 cloud lib/testing/k6-nfl-sunday.js",
    "load-test:docker": "docker run -i --rm -v $(pwd)/lib/testing:/scripts grafana/k6 run /scripts/k6-nfl-sunday.js",
    "security:verify": "tsx scripts/verify-secrets.ts",
    "security:audit": "tsx scripts/supabase-security-audit.ts",
    "security:management": "tsx scripts/supabase-management-security.ts",
    "security:rls:check": "tsx scripts/check-rls-status.ts",
    "security:rls:enable": "echo 'Run scripts/enable-comprehensive-rls.sql in Supabase SQL Editor'",
    "security:monitor": "tsx scripts/monitor-security.ts",
    "security:monitor:live": "tsx scripts/monitor-security-live.ts",
    "security:test": "tsx scripts/test-rls-security.ts",
    "verify:secrets": "tsx scripts/verify-secrets.ts",
    "db:status": "tsx scripts/check-database-status.ts",
    "precommit": "npm run security:check && npm run lint && npm run type-check"
