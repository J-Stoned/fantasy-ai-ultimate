# MARCUS "THE FIXER" RODRIGUEZ - MOBILE ENV CONFIG
# Copy this to .env.local and fill in your values

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# API Configuration  
EXPO_PUBLIC_API_URL=https://api.fantasyai.app
EXPO_PUBLIC_WEBSOCKET_URL=wss://ws.fantasyai.app

# Sports Data Provider
EXPO_PUBLIC_SPORTS_API_KEY=your-mysportsfeeds-key

# Revenue Cat (for subscriptions)
EXPO_PUBLIC_REVENUE_CAT_KEY_IOS=your-ios-key
EXPO_PUBLIC_REVENUE_CAT_KEY_ANDROID=your-android-key

# Sentry (error tracking)
EXPO_PUBLIC_SENTRY_DSN=your-sentry-dsn

# Feature Flags
EXPO_PUBLIC_ENABLE_VOICE=true
EXPO_PUBLIC_ENABLE_GPU_OPTIMIZER=true
EXPO_PUBLIC_ENABLE_3D_VISUALIZATION=true
EXPO_PUBLIC_ENABLE_AR_STATS=true

# Environment
EXPO_PUBLIC_ENV=production